{
  //"list.source.files.args": ["--hidden", "--files", "-uu"],
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.grep.args": ["--hidden"],
  "list.source.mru.maxLength": 4096,
  "list.height": 20,
  "suggest.echodocSupport": true,
  "suggest.noselect": false,
  "suggest.fixInsertedWord": true,
  "suggest.removeDuplicateItems": true,
  "vetur.format.enable": true,
  "vetur.format.options.tabSize": 2,
  "codeLens.enable": true,
  "coc.preferences.formatOnSaveFiletypes": ["dart"],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "filetypes": [
        "go"
      ]
    }
  },
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ]
}
